{
    "name": "struzik-vladislav/epp-ext-hostmasterua-uaepp",
    "description": "UAEPP extension provided by Hostmaster (https://hostmaster.ua/).",
    "keywords": ["epp", "extension", "hostmaster", "registry", "uaepp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Struzik",
            "email": "vladislav@struzik.blog"
        }
    ],
    "require": {
        "php": ">=7.4 <8.4",
        "ext-dom": "*",
        "psr/log": "~1.0 | ~2.0 | ~3.0",
        "struzik-vladislav/epp-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Struzik\\EPPClient\\Extension\\HostmasterUA\\UAEPP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Struzik\\EPPClient\\Tests\\": "vendor/struzik-vladislav/epp-client/tests/",
            "Struzik\\EPPClient\\Extension\\HostmasterUA\\UAEPP\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests/"
    }
}
